<form method="post" action="{{ path('participation_delete', {'idParticipation': participation.idParticipation}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ participation.idParticipation) }}">
    <button class="btn">Delete</button>
</form>
